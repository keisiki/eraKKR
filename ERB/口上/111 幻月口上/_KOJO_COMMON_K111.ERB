#DIM CONST 처음의B절정 = 240
#DIM CONST 처음의M절정 = 241

;이름의 정의
겐게츠 = NAME_TO_CHARA("겐게츠")
겐게츠_대상 = MASTER
;[공허]상태의 경우, 입이 막히고 있는 경우는 구상을 표시하지 않는다
IF TALENT:공허 || IS_EQUIP_CONTINUE(TARGET, "입장착")
	RETURN 0
ENDIF
;-------------------------------------------------
;첫절정은 재사용할 수 있을 것 같은 것으로 여기에 작성
;-------------------------------------------------
;-------------------------------------------------
;처음Ｃ절정
;-------------------------------------------------
IF NOWEX:Ｖ절정 > 0
	;처음의 C절정
	IF CFLAG:TARGET:처음의C절정 == 0
		CFLAG:TARGET:처음의C절정 += 1
		PRINTFORMW 「응응... !!」
		PRINTFORMW %ANAME(겐게츠)%는 몸을 흠칫 시키고 달했다
		PRINTFORMW 「타인에게 가게 되면, 뭔가 부끄럽네요...」
	;다음번 이후
	ELSE
		CFLAG:TARGET:처음의C절정 += 1
		;PRINTFORMW 
	ENDIF
ENDIF

;-------------------------------------------------
;처음Ｖ절정
;-------------------------------------------------
IF NOWEX:Ｖ절정 > 1
	;처음의 V절정
	IF CFLAG:TARGET:처음의V절정 == 0
		CFLAG:TARGET:처음의V절정 += 1
		PRINTFORMW 「익크!」
		PRINTFORMW 「읏─!!」
		PRINTFORMW %ANAME(겐게츠)%는 %ANAME(겐게츠_대상)%의 손에 의해 가게 되었다...
		PRINTFORMW 「당신 상당히, 능숙해요...」
		PRINTFORMW %ANAME(겐게츠)%는 얼굴을 상기 시키면서 그렇게 중얼거렸다...
	;다음번 이후
	ELSE
		CFLAG:TARGET:처음의V절정 += 1
		;PRINTFORMW 
	ENDIF
ENDIF

;-------------------------------------------------
;처음Ａ절정
;-------------------------------------------------
IF NOWEX:Ａ절정 > 1
	;처음의 A절정
	IF CFLAG:TARGET:처음의A절정 == 0
		CFLAG:TARGET:처음의A절정 += 1
		PRINTFORMW 「크루!! 와 버리는 것!」
		PRINTFORMW 「─!!」
		PRINTFORMW 「서, 설마 엉덩이로 가게 되다니...」
		PRINTFORMW %ANAME(겐게츠)%는 어깨로 숨을 쉬면서 계속했다
		PRINTFORMW 「나의 몸, 원래로 돌아올까나?」
		PRINTFORMW 「이제(이미/벌써/더는/정말), 뒤늦음일까...」
		PRINTFORMW 그렇게 중얼거리는 %ANAME(겐게츠)%였지만, 그 구멍은 실룩거림
		PRINTFORML 다음의 삽입을 애타게 기다리고 있다...
		PRINTFORMW
	;다음번 이후
	ELSE
		CFLAG:TARGET:처음의A절정 += 1
		;PRINTFORMW 
	ENDIF
ENDIF

;-------------------------------------------------
;처음Ｂ절정
;-------------------------------------------------
IF NOWEX:Ｂ절정 > 1
	;처음의 B절정
	IF CFLAG:TARGET:처음의B절정 == 0
		CFLAG:TARGET:처음의B절정 += 1
		;PRINTFORMW 
	;다음번 이후
	ELSE
		CFLAG:TARGET:처음의B절정 += 1
		;PRINTFORMW 
	ENDIF
ENDIF

;-------------------------------------------------
;처음Ｍ절정
;-------------------------------------------------
IF NOWEX:Ｍ절정 > 1
	;처음의 M절정
	IF CFLAG:TARGET:처음의M절정 == 0
		CFLAG:TARGET:처음의M절정 += 1
		;PRINTFORMW 
	;다음번 이후
	ELSE
		CFLAG:TARGET:처음의M절정 += 1
		;PRINTFORMW 
	ENDIF
ENDIF

