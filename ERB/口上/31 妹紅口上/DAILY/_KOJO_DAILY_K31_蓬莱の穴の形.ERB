	RETURN 0

SIF IS_LOVER(대상)
	RETURN 0

;대상이 군주라면 안 돼
SIF GET_COUNTRY_BOSS(CFLAG:대상:소속) == 대상
	RETURN 0

;자객에서 강간하고 있는 것
SIF KDVAR:대상:모코우_자객 != 2
	RETURN 0

;끝내면 나오지 않는다
SIF KDVAR:대상:모코우_구멍 == -1
	RETURN 0


RETURN CHECK_KOJO_DAILY_HAPPEN(대상, 0, 0)

